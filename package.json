{
  "name": "Pong-OOP-vs-FP",
  "description": "Pong game implemented twice (with OOP and FP)",
  "author": "Rafał Łużyński <rafalluzynski0@gmail.com>",
  "license": "(CC-BY-NC-SA-4.0)",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mindbrave/pong-oop-vs-fp.git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.19",
    "@types/chai": "^4.1.0",
    "@types/core-js": "^0.9.44",
    "@types/cucumber": "^3.2.1",
    "@types/jest": "^22.0.1",
    "@types/ramda": "^0.25.16",
    "chai": "^4.1.2",
    "cucumber": "^3.2.0",
    "cucumber-pretty": "^1.3.2",
    "jest": "^22.1.1",
    "systemjs": "^0.20.19",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0"
  },
  "scripts": {
    "test-fp": "jest ./src/fppong/**/*.test.ts; ./node_modules/.bin/cucumber.js -f node_modules/cucumber-pretty --compiler ts:ts-node/register ./src/fppong/;",
    "test-oop": "jest ./src/ooppong/**/*.test.ts; ./node_modules/.bin/cucumber.js -f node_modules/cucumber-pretty --compiler ts:ts-node/register ./src/ooppong/tests/acceptance/;",
    "build": "./node_modules/.bin/tsc --module system"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./node_modules/"
    ],
    "testRegex": "(.*\\.test)\\.(ts)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "pipe-operator": "0.0.7",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.6",
    "typescript": "^2.6.2"
  }
}
